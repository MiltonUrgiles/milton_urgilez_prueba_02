<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editTextNombres"
        android:layout_width="339dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:enabled="false"
        android:inputType="text"
        android:text="Nombres"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextApellidos"
        android:layout_width="339dp"
        android:layout_height="40dp"
        android:ems="10"
        android:enabled="false"

        android:inputType="text"
        android:text="Apellidos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNombres" />

    <EditText
        android:id="@+id/editTextDividendo"
        android:layout_width="339dp"
        android:layout_height="40dp"
        android:ems="10"
        android:enabled="false"

        android:inputType="text"
        android:text="Dividendo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextApellidos" />

    <EditText
        android:id="@+id/editTextDivisor"
        android:layout_width="339dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="text"
        android:enabled="false"

        android:text="Divisor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextDividendo" />

    <EditText
        android:id="@+id/editTextParteEntera"
        android:layout_width="339dp"
        android:layout_height="40dp"
        android:ems="10"
        android:enabled="false"

        android:inputType="text"
        android:text="Parte entera"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextDivisor" />

    <EditText
        android:id="@+id/editTextNumeroInvertido"
        android:layout_width="339dp"
        android:layout_height="40dp"
        android:ems="10"
        android:enabled="false"

        android:inputType="text"
        android:text="Num. Invertido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextResiduo" />

    <EditText
        android:id="@+id/editTextResiduo"
        android:layout_width="339dp"
        android:layout_height="40dp"
        android:ems="10"
        android:enabled="false"

        android:inputType="text"
        android:text="Residuo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextParteEntera" />

    <Button
        android:id="@+id/buttonSiguiente"
        android:layout_width="353dp"
        android:layout_height="47dp"
        android:layout_marginTop="240dp"
        android:onClick="Siguiente"

        android:text="Siguiente"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.379"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumeroInvertido" />

    <Button
        android:id="@+id/buttonMostrarResultados"
        android:layout_width="353dp"
        android:layout_height="47dp"
        android:text="MostrarResultados"
        android:enabled="false"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.379"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonSiguiente" />

</androidx.constraintlayout.widget.ConstraintLayout>